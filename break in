-- Gui to Lua
-- Version: 3.2

-- Instances:

local ScreenGui = Instance.new("ScreenGui")
local TextButton = Instance.new("TextButton")
local UICorner = Instance.new("UICorner")

--Properties:

ScreenGui.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")
ScreenGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling

TextButton.Parent = ScreenGui
TextButton.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
TextButton.BorderColor3 = Color3.fromRGB(0, 0, 0)
TextButton.BorderSizePixel = 0
TextButton.Position = UDim2.new(0.0760956183, 0, 0.261437923, 0)
TextButton.Size = UDim2.new(0.0796812773, 0, 0.163398698, 0)
TextButton.Font = Enum.Font.SourceSans
TextButton.Text = "Toggle"
TextButton.TextColor3 = Color3.fromRGB(245, 245, 245)
TextButton.TextSize = 30.000
TextButton.Draggable = true
TextButton.MouseButton1Down:Connect(function()
	game:GetService("VirtualInputManager"):SendKeyEvent(true, "F6" , false , game)
end)

UICorner.Parent = TextButton

local DarkraiX = loadstring(game:HttpGet("https://raw.githubusercontent.com/GamingScripter/Kavo-Ui/main/Darkrai%20Ui", true))()

local Library = DarkraiX:Window("Bacon hub","break in","by sigma balls",Enum.KeyCode.F6);

--[[
DarkraiX:Window(
1 = break in (string)
2 = Game Name (You Can Keep This Empty To Get The Current Game Name!) (string)
3 = A Logo If You Have One! (string)
4 = Ui Library Toggle (I'll Not Prefer Touching It) (function)
);
]]

Tab1 = Library:Tab("Main")

--[[
Library:Tab(
1 = Items (string)
)
]]
